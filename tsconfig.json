{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": true,
    "inlineSourceMap": true,
    "noEmitOnError": false,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "strict": true,
    "lib": ["es2017"]
  },
  "exclude": ["node_modules"],
  "include": ["*.ts", "*.d.ts"]
}
